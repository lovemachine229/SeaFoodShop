@model List<Services.ViewModels.EmployeeViewModel>

@foreach (var item in Model)
{
    <tr>
        <td>@item.Email</td>
        <td>@item.FullName</td>
        <td>@item.RoleName</td>
        <td><input type="checkbox" class="cbStatus" @(item.IsActive == true ? "checked" : "") data-id="@item.Id" /></td>
        <td><a href="/Employee/Edit/@item.Id">Sửa</a> | <a href="#" class="employee_delete" data-id="@item.Id">Xóa</a></td>
    </tr>
}
